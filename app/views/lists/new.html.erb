<%# アプリのヘッダー部分 %>
<%= render "shared/header" %>
<%# //アプリのヘッダー部分// %>

<%# topページのbody部分 %>
<div class="new-list">
    <h3 class="new-list-top">リスト作成
      <input type="submit" value="リストを共有する" class="ok-buttan">
    </h3>
    <form action="#" method="post" name="title-form" class="new-title">
      タイトル：<input type="text" placeholder="to Doリストのタイトル">
    </form>
    <%# 共有するメンバーを検索する %>
    <div class="new-member">
      誰と共有する？
    </div>
    <%# //共有するメンバーを検索する// %>
    <form action="#" method="post" name="item-form" class="new-item">
      項目：<input type="text" >
    </form>
    <%# 追加したアイテムを一覧にして表示する %>
    <div class="item-lineup">
      アイテム①
    </div>  
    <%# //追加したアイテムを一覧にして表示する// %>
</div>
<%# //topページのbody部分// %>

<%# アプリのフッター部分 %>
<%= render "shared/footer" %>
<%# //アプリのフッター部分// %>
