<%# アプリのヘッダー部分 %>
<%= render "shared/header" %>
<%# //アプリのヘッダー部分// %>

<%# topページのbody部分 %>
<div class="new-list">
    <h1 class="new-list-top">リスト新規作成</h1>

    <%# リストのタイトルを入力 %>
    <%= form_with model: @list, class: "new-title" do |form| %>
      リストのタイトルは？<%= form.text_field :title %>
      <%= form.submit 'リストを作成' %>
    <%end%>
    <%# //リストのタイトルを入力// %>

    <%# 共有するメンバーを検索する %>
    <div class="new-member">
      誰と共有する？
    </div>
    <%# //共有するメンバーを検索する// %>

</div>
<%# //topページのbody部分// %>

<%# アプリのフッター部分 %>
<%= render "shared/footer" %>
<%# //アプリのフッター部分// %>
